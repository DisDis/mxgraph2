<mxStylesheet>
	<add as="defaultVertex">
		<add as="shape" value="rectangle"/>
		<add as="perimeter" value="rectanglePerimeter"/>
		<add as="fontSize" value="11"/>
		<add as="align" value="center"/>
		<add as="verticalAlign" value="middle"/>
		<add as="fontColor" value="black"/>
		<add as="strokeColor" value="black"/>
		<add as="fillColor" value="white"/>
		<add as="overflow" value="fill"/>
	</add>
	<add as="defaultEdge">
		<add as="shape" value="connector"/>
		<add as="elbow" value="vertical"/>
		<add as="fontSize" value="11"/>
		<add as="align" value="center"/>
		<add as="fontColor" value="black"/>
		<add as="verticalAlign" value="bottom"/>
		<add as="strokeColor" value="black"/>
	</add>
	<add as="lan" extend="defaultVertex">
		<add as="shape" value="lan"/>
		<add as="perimeter" value="lanPerimeter"/>
		<add as="fillColor" value="none"/>
	</add>
	<add as="ase" extend="defaultVertex">
		<add as="shape" value="ase"/>
		<add as="routingCenterX" value="0.4"/>
		<add as="spacingTop" value="2"/>
	</add>
	<add as="ima" extend="defaultVertex">
		<add as="shape" value="triangle"/>
		<add as="perimeter" value="trianglePerimeter"/>
	</add>
	<add as="network" extend="defaultVertex">
		<add as="shape" value="cloud"/>
		<add as="perimeterSpacing" value="0"/>
	</add>
	<add as="router" extend="defaultVertex">
		<add as="align" value="left"/>
		<add as="verticalAlign" value="top"/>
	</add>
	<add as="isr" extend="defaultVertex">
		<add as="routingCenterX" value="0.25"/>
	</add>
	<add as="label">
		<add as="shape" value="rectangle"/>
		<add as="fontSize" value="11"/>
		<add as="align" value="center"/>
		<add as="verticalAlign" value="middle"/>
		<add as="fontColor" value="black"/>
		<add as="spacingRight" value="2"/>
		<add as="spacingLeft" value="2"/>
		<add as="overflow" value="visible"/>
	</add>
	<add as="title" extend="label">
		<add as="verticalAlign" value="bottom"/>
	</add>
	<add as="details" extend="label">
		<add as="verticalAlign" value="top"/>
	</add>
	<add as="sourceLabel" extend="title">
		<add as="align" value="left"/>
	</add>
	<add as="targetLabel" extend="details">
		<add as="align" value="right"/>
	</add>
	<add as="carrier" extend="defaultEdge">
		<add as="edgeStyle" value="carrierNetwork"/>
	</add>
	<add as="demarcation" extend="defaultEdge">
		<add as="dashed" value="1"/>
		<add as="align" value="left"/>
		<add as="verticalAlign" value="top"/>
	</add>
	<add as="vsat" extend="defaultVertex">
		<add as="shape" value="image"/>
		<add as="image" value="../../images/vsat.png"/>
	</add>
	<add as="endpointLocation" extend="defaultVertex">
		<add as="overflow" value="none"/>
		<add as="dashed" value="1"/>
		<add as="strokeColor" value="#00FF00"/>
		<add as="fillColor" value="none"/>
		<add as="spacing" value="6"/>
		<add as="verticalAlign" value="bottom"/>
		<add as="fontStyle" value="1"/>
	</add>
	<add as="remoteLocation" extend="endpointLocation">
		<add as="verticalAlign" value="top"/>
	</add>
</mxStylesheet>
